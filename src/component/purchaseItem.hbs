<div>
	<div class="card p-1">
		<div class="row">
			<div class="col-4 mt-3">
				<div class="my-2">
					{{number}}
				</div>
				<div class="my-2">
					<h6>{{status}}</h6>
				</div>
			</div>
			<div class="col-2  mt-3">
				<div class="my-2">
					{{title}}
				</div>
				<div class="my-2">
					{{value}}
				</div>
			</div>
			<div class="col-4 p-1">
				{{#each images}}
					<img class=" " src="{{this}}" alt="" width="30%">
				{{/each}}

			</div>
			<div class="col-2 mt-4">
				<a class="btn {{#if isPrimary}}btn-primary{{/if}}" href="{{link}}">{{text}}</a>
			</div>
		</div>
	</div>
</div>