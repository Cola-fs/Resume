<div class="card">
    <div class="card-body">
        {{#with heading}}

        <div class="card">
            <div class="card-body">


                {{#with main}}
                <h1> {{title}} </h1>
                <p> {{text}} </p>
                {{#with button_text}}

                <button class="btn btn-outline-primary"> {{solid}} </button>
                <button class="btn btn-outline-primary"> {{outline}} </button>

                {{/with}}
                {{/with}}


            </div>

        </div>
        <br>
        <div class="card">
            <div class="card-body">

                {{#with second}}
                <h2> {{title}} </h1>
                    <p> {{text}} </p>
                    {{#with button_text}}
                    <div>
                        <button class="btn btn-secondary"> {{solid}} </button>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-secondary"> {{outline}} </button>
                    </div>
                    {{/with}}
                    {{/with}}


            </div>
        </div>
        <br>
        <div class="card">
            <div class="card-body">

                {{#with subblock}}
                <h3> {{title}} </h3>
                <p> {{text}} </p>
                {{#with button_text}}
                <div>
                    <button class="btn btn-dark"> {{solid}} </button>
                </div>
                <br>
                <div>
                    <button class="btn btn-outline-dark"> {{outline}} </button>
                </div>
                {{/with}}
                {{/with}}


            </div>
        </div>
        <br>
        {{/with}}
    </div>
</div>
<br><br>

<div class="card">
    <div class="card-body">
        {{#with nested_display}}
        <div class="display-1"> {{title}} </div>

        {{#with children_display}}
        <div class="display-4"> {{title}} </div>
        <p> {{text}} </p>
        {{#with button }}
        <button class="btn btn-outline-warning"> {{text}} </button>
        {{/with}}
        {{/with}}

        <h6> {{header_text}} </h6>
        {{#with button }}
        <button class="btn btn-info"> {{text}} </button>
        {{/with}}

        {{/with}}
    </div>
</div>

<br>

<div class="card">
    <div class="card-body">
        {{#with paragraph}}
        <div class="text-uppercase">
            {{title}}
        </div>
        <div class="text-lowercase">
            {{text}}
        </div>
        {{#with button }}
        <a href="{{url}}" class="btn btn-link"> {{text}} </a>
        {{/with}}
        <br>
        <div class="card">
            <div class="card-body">
                {{#each list_name }}
                <ul>
                    <li>
                        <a href="{{url}}" class="btn btn-link"> {{text}} </a>
                    </li>
                </ul>
                {{/each}}
            </div>
        </div>


        {{/with}}
    </div>
</div>