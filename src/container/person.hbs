<div>
    {{#with person}}
    <section>
        <h1> {{name}} </h1>
        <div>
            <b>Age: {{age}}</b>
        </div>
        <br>
        <div>
            Sex: {{gender}}
        </div>
        <br>
        <div>
            {{#with address}}
            <address> {{country}} {{zip}} {{state}}
                <br>
                {{city}} {{street}}
            </address>
            {{/with}}
        </div>
    </section>
    <br>
    <hr>
    <section>
        {{> title data="Education" }}
        {{#each education}}
        <ul>
            <li>
                {{university}} {{graduationYear}}
                <br>
                {{major}}
                <br>
                {{degree}}
            </li>
        </ul>
        {{/each}}
    </section>
    <br>
    <hr>
    <section>
        {{> title data="Work experience" }}
        {{#each workExperience}}
        <ul>
            <li>
                <div>
                    <em>{{title}}</em>
                </div>
                <br>
                <div>
                    {{startDate}} -
                    {{#if endDate}} {{endDate }} {{else}} (досі працюємо) {{/if}}
                </div>
                <br>
                <div>
                    {{#each responsibilities}}
                    <ul>
                        <li>
                            {{this}}
                        </li>
                    </ul>
                    {{/each}}
                </div>
                <br>
                <div>
                    <b> {{company}} {{industry}} </b> ({{year_founded}})
                </div>
                <br>
                <div>
                    Employees
                </div>
                <br>
                <div>
                    {{#each employees}}
                    <ul>
                        <li>
                            <div> <b> {{name}} </b> </div>
                            <div> {{position}} </div>
                            <div> {{department}} </div>
                            <br>
                            <div> <b>Projects:</b> </div>
                            <br>
                            <div>
                                {{#each projects}}
                                <ul>
                                    <li>
                                        <div> <b> {{name}} </b> ({{status}}) </div>
                                        <br>
                                        <div> {{description}} </div>
                                        <br>
                                        <div> <b> Teams: </b></div>
                                        <br>
                                        {{#each teams}}
                                        <ul>
                                            <li>
                                                <div> <b> {{team_name}} </b> </div>
                                                <br>
                                                <div> <b> Team leader </b> </div>
                                                <br>
                                                {{#with team_leader}}
                                                <ul>
                                                    <li>
                                                        <div> {{name}} </div>
                                                        <div> {{title}} </div>
                                                        <div> {{email}} </div>
                                                    </li>
                                                </ul>
                                                {{/with}}
                                                <br>
                                                <div> <b> Team members </b> </div>
                                                {{#each team_members}}
                                                <br>
                                                <ul>
                                                    <li>
                                                        <div> {{name}} </div>
                                                        <div> {{title}} </div>
                                                        <div> {{email}} </div>
                                                        <br>
                                                        <div> <b> Skills: </b> </div>
                                                        {{#each skills}}
                                                        <ul>
                                                            <li>
                                                                {{this}}
                                                            </li>
                                                        </ul>
                                                        {{/each}}
                                                        <br>
                                                        <div> <b>Projects:</b> </div>
                                                        <br>
                                                        {{#each projects}}
                                                        <ul>
                                                            <li>
                                                                <div> <b> {{name}} </b> </div>
                                                                <br>
                                                                <div> {{description}} </div>
                                                                <br>
                                                                <div> <b> Technologies: </b> </div>
                                                                <div>
                                                                    {{#each technologies}}
                                                                    <ul>
                                                                        <li>
                                                                            {{this}}
                                                                        </li>
                                                                    </ul>
                                                                    {{/each}}
                                                                </div>
                                                                <br>
                                                                <div> <b> Team members </b> </div>
                                                                 <br>
                                                                {{#each team_members}}
                                                                <ul>
                                                                    <li>
                                                                        <div> {{name}} </div>
                                                                        <div> {{title}} </div>
                                                                         <br>
                                                                    </li>
                                                                </ul>
                                                                {{/each}}

                                                            </li>
                                                        </ul>


                                                        {{/each}}


                                                    </li>
                                                </ul>
                                                {{/each}}
                                            </li>
                                        </ul>


                                        {{/each}}

                                    </li>
                                </ul>
                                {{/each}}
                            </div>
                        </li>
                    </ul>
                    {{/each}}
                </div>
                <br>

            </li>
        </ul>
        {{/each}}
    </section>
    
    <hr>
    {{/with}}



</div>