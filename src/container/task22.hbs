<div class="text-center">
    {{#with page_header}}
    <h1> {{title}} </h1>
    <p class="text-uppercase"> {{description}} </p>
    {{/with}}
</div>

<div class="card">
    <div class="card-body">
        {{#with section}}

        {{#with about_us}}

        {{#with title_list}}
        <div class="text-end">
            <div class="display-6">
                {{main}}
            </div>
            <br>
            <p> {{sub}} </p>
        </div>


        {{/with}}

        {{#with button}}
        <button class="btn btn-outline-info">{{text}} </button>
        {{/with}}

        {{/with}}




    </div>
</div>
<br>
<div class="card">
    <div class="card-body">

        {{#with advantage}}
        <div class="display-5"> {{title}} </div>
        <br>

        <div class="card">
            <div class="card-body">

                {{#with efficiency}}
                <div class="display-4"> {{title}} </div>
                <h6> {{sub_title}} </h6>
                <p> {{description}} </p>
                <button class="btn btn-outline-warning">{{button}} </button>
                {{/with}}

            </div>
        </div>
        <br>
        <div class="card">
            <div class="card-body">
                <div class="text-center">
                    {{#with innovation}}
                    <div class="display-4"> {{title}} </div>
                    <h6> {{sub_title}} </h6>
                    <p> {{description}} </p>
                </div>

                <button class="btn btn-outline-warning">{{button}} </button>
                {{/with}}

            </div>
        </div>
        <br>
        <div class="card">
            <div class="card-body">
                <div class="text-end">
                    {{#with collaboration}}
                    <div class="display-4"> {{title}} </div>
                    <h6> {{sub_title}} </h6>
                    <p> {{description}} </p>
                </div>

                <button class="btn btn-outline-warning">{{button}} </button>
                {{/with}}

            </div>
        </div>

        {{/with}}
        <br>

        <div class="card">
            <div class="card-body">

                {{#with agreement}}
                <div class="display-5"> {{title}} </div>

                <div class="card">
                    <div class="card-body">

                        {{#with info}}

                        <h5> {{sub_title}} </h5>

                        <div class="card">
                            <div class="card-body">
                                {{#with detail}}
                                <p> {{text}} </p>
                                <button class="btn btn-success"> {{button}} </button>
                                {{/with}}



                            </div>
                        </div>
                        {{/with}}


                    </div>
                </div>

                {{/with}}

            </div>
        </div>



    </div>
</div>


{{/with}}